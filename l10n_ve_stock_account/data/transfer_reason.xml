<odoo>
    <data noupdate="0">
        <!-- Donation -->
        <record id="transfer_reason_donation" model="transfer.reason">
            <field name="name">Donación</field>
            <field name="code">donation</field>
            <field name="active">True</field>
        </record>

        <!-- Sale -->
        <record id="transfer_reason_sale" model="transfer.reason">
            <field name="name">Venta</field>
            <field name="code">sale</field>
            <field name="active">True</field>
        </record>

        <!-- Transfer between warehouses -->
        <record id="transfer_reason_transfer_between_warehouses" model="transfer.reason">
            <field name="name">Traslado entre almacenes</field>
            <field name="code">transfer</field>
            <field name="active">True</field>
        </record>

        <!-- Export -->
        <record id="transfer_reason_export" model="transfer.reason">
            <field name="name">Exportación</field>
            <field name="code">export</field>
            <field name="active">True</field>
        </record>

        <!-- Self Consumption -->
        <record id="transfer_reason_self_consumption" model="transfer.reason">
            <field name="name">Autoconsumo</field>
            <field name="code">self_consumption</field>
            <field name="active">True</field>
        </record>

        <!-- Consignment -->
        <record id="transfer_reason_consignment" model="transfer.reason">
            <field name="name">Consignación</field>
            <field name="code">consignment</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
