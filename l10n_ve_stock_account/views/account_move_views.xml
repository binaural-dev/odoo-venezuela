<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_move_inherit_form" model="ir.ui.view">
        <field name="name">account_move_inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="stock_move_invoice.account_move_form_view_inherited"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='transfer_ids']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>

            <xpath expr="//field[@name='picking_id']" position="after">
                <field name="guide_number" />
            </xpath>

            <xpath expr="//field[@name='picking_id']" position="attributes">
                <attribute name="readonly">1</attribute>
            </xpath>

        </field>
    </record>
</odoo>
