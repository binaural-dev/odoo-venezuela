<odoo>
    <record id="view_stock_warehouse_inherit_l10n_ve_stock_account" model="ir.ui.view">
        <field name="name">stock.warehouse.form.inherit.l10n.ve.stock.account</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="readonly_is_consignation_warehouse" invisible="1"/>
                <field name="is_consignation_warehouse" readonly="readonly_is_consignation_warehouse" />
            </xpath>
        </field>
    </record>
    <record id="view_stock_warehouse_tree_inherit_l10n_ve_stock_account" model="ir.ui.view">
        <field name="name">stock.warehouse.tree.inherit.l10n.ve.stock.account</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='partner_id']" position="after">
                <field name="is_consignation_warehouse" optional="hide"/>
            </xpath>
        </field>
    </record>
</odoo>
