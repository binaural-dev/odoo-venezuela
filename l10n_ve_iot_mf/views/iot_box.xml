
<odoo>
    <record id="iot_box_view_form" model="ir.ui.view">
        <field name="name">iot.box.view.form</field>
        <field name="model">iot.box</field>
        <field name="inherit_id" ref="iot.iot_box_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ip_url']" position="after">
                <field name="blacklist"/>
                <field name="blacklist_port_ids" invisible="not blacklist"/>
                <!-- <field name="ip_public"/> -->
                <field name="has_fiscal_machine"/>
                <field name="fiscal_port_ids" invisible="not has_fiscal_machine"/>
            </xpath>
        </field>
    </record>
</odoo>
