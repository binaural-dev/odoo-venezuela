<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_l10n_ve_contact" model="ir.ui.view">
            <field name="name">res.config.settings.binaural.contact</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="100"/>
            <field name="inherit_id" ref="l10n_ve_rate.res_config_settings_l10n_ve_rate"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='l10n_ve_rate_settings']" position="after">
                    <h2>Contact Configuration</h2>
                    <div class="row mt16 o_settings_container" name="l10n_ve_contact_settings_container">
                        <div id="partner_section" class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <div class="text-muted">
                                    Validate user creation by company
                                </div>
                                <div class="content-group">
                                    <div class="mt16">
                                        <field name="validate_user_creation_by_company"/>
                                    </div>
                                </div>
                            </div>
                            <div class="o_setting_right_pane">
                                <div class="text-muted">
                                    Validate user creation general
                                </div>
                                <div class="content-group">
                                    <div class="mt16">
                                        <field name="validate_user_creation_general"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>

            </field>
    </record>

</odoo>
