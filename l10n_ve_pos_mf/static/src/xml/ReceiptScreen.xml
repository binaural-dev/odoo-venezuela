<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="binaural_pos_mf.ReceiptScreen" t-inherit="point_of_sale.ReceiptScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[1]/div[1]/div[2]/div[1]/div[1]" position="before">
            <div t-if="currentOrder.mf_invoice_number" class="d-flex flex-column m-4 mb-0">
                <p>Nro Factura: <t t-out="currentOrder.mf_invoice_number"/></p>
                <p t-if="currentOrder.fiscal_machine">Serial de la Maquina fiscal: <t t-out="currentOrder.fiscal_machine"/></p>
                <p t-if="currentOrder.mf_reportz">Pertece al siguiente Z: <t t-out="currentOrder.mf_reportz"/></p>
            </div>
        </xpath>
    </t>
</templates>
