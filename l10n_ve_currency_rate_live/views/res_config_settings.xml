<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_inherit_currency_rate_live" model="ir.ui.view">
            <field name="name">res.config.settings.binaural.rate.update</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="currency_rate_live.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@class='mt8 row']" position="after">
                    <div style="margin-top: 4%">
                        <field name="can_update_habil_days"/>
                        <span><b>only update in habil days</b></span>
                    </div>   
                </xpath>
            </field>
        </record>
    </data>
</odoo>