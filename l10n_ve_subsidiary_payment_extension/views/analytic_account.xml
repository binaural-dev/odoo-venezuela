
<odoo>
    <record id="view_account_analytic_account_subsidiary_form" model="ir.ui.view">
        <field name="name">l10n.ve.subsidiary.payment.extension.view.account.analytic.account.subisidiary.form</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="l10n_ve_subsidiary.view_account_analytic_account_subsidiary_form"/>
        <field name="arch" type="xml">
	    <field name="code" position="after">
		<field name="municipality_id" required="use_subsidiary_with_multiple_municipalities"/>
		<field name="municipal_supplier_retentions_sequence_id"/>
	    </field>
	    <sheet position="inside">
		<notebook>
		    <page name="tax_authorities_data" string="Tax Authorities Data" invisible="not use_subsidiary_with_multiple_municipalities">
			<field name="use_subsidiary_with_multiple_municipalities" invisible="1"/>
			<group>
			    <group>
				<field name="tax_authorities_name" required="use_subsidiary_with_multiple_municipalities"/>
                            	<field name="economic_activity_number" required="use_subsidiary_with_multiple_municipalities"/>
		    	    </group>
			    <group>
				<field name="tax_authorities_logo" widget="image" class="oe_avatar" required="use_subsidiary_with_multiple_municipalities"/>
			    </group>
                    	</group>
		    </page>
		</notebook>
	    </sheet>
        </field>
    </record>
</odoo>
