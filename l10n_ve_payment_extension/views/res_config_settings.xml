<odoo>
    <data>
        <record id="res_config_settings_l10n_ve_payment_extension" model="ir.ui.view">
            <field name="name">res.config.settings.l10n.ve.payment.extension</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="100"/>
            <field name="inherit_id" ref="l10n_ve_rate.res_config_settings_l10n_ve_rate"/>
            <field name="arch" type="xml">
		<xpath expr="//div[@name='l10n_ve_rate_settings']" position="after">
                <h2>Retention Journals</h2>
                <div class="row mt16 o_settings_container" name="l10n_ve_retention_journals_settings">
                    <div class="o_setting_right_pane">
                        <group colspan="4" col="4">
                            <field name="iva_supplier_retention_journal_id"/>
                            <field name="iva_customer_retention_journal_id"/>
                            <field name="islr_supplier_retention_journal_id"/>
                            <field name="islr_customer_retention_journal_id"/>
                            <field name="municipal_supplier_retention_journal_id"/>
                            <field name="municipal_customer_retention_journal_id"/>
                            <field name="code_visible"/>
                        </group>
                    </div>
                </div>
                <h2>Other withholding settings</h2>
                <div class="row mt16 o_settings_container" name="l10n_ve_withholding_settings">
                    <div class="o_setting_right_pane">
                        <group colspan="4" col="4">
                            <field name="condition_withholding_id"/>
                        </group>
                    </div>
                </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
