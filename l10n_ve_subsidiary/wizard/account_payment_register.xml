
<odoo>
    <data>
        <record id="account_payment_register_form" model="ir.ui.view">
            <field name="name">account.payment.register.form.l10n.ve.subsidiary</field>
            <field name="model">account.payment.register</field>
            <field name="inherit_id" ref="account.view_account_payment_register_form"/>
            <field name="arch" type="xml">
                <field name="payment_method_line_id" position="after">
                    <field name="company_subsidiary" invisible='1'/>   
                    <field name="account_analytic_id"
                        invisible="not company_subsidiary"
                        required="company_subsidiary"/>
                </field>
            </field>
        </record>
    </data>
</odoo>
