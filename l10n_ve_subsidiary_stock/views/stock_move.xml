
<odoo>

    <record id="view_move_search" model="ir.ui.view">
        <field name="name">l10n.ve.subsidiary.stock.move.search</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_search"/>
        <field name="arch" type="xml">
	    <field name="partner_id" position="after">
		<field name="subsidiary_origin_id"/>
		<field name="subsidiary_dest_id"/>
	    </field>
	    <filter name="today" position="after">
		<filter name="subsidiary_origin" string="Has Origin Subsidiary" domain="[('subsidiary_origin_id', '!=', False)]"/>
		<filter name="subsidiary_dest" string="Has Destination Subsidiary" domain="[('subsidiary_dest_id', '!=', False)]"/>
	    </filter>
	    <group position="inside">
		<filter string="Origin Subsidiary" name="subsidiary_origin_id" domain="[]" context="{'group_by': 'subsidiary_origin_id'}"/>
		<filter string="Destination Subsidiary" name="subsidiary_dest_id" domain="[]" context="{'group_by': 'subsidiary_dest_id'}"/>
	    </group>
        </field>
    </record>

    <record id="view_move_tree" model="ir.ui.view">
        <field name="name">l10n.ve.subsidiary.stock.move.tree</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
	    <field name="date" position="after">
		<field name="subsidiary_origin_id" optional="hide"/>
		<field name="subsidiary_dest_id" optional="hide"/>
	    </field>
        </field>
    </record>

</odoo>
